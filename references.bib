@article{Piotrowski2019_HistoricalModelsSerial,
  title = {Historical {{Models}} and {{Serial Sources}}},
  author = {Piotrowski, Michael},
  year = {2019},
  month = jun,
  journal = {Journal of European Periodical Studies},
  volume = {4},
